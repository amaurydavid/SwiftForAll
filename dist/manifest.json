{
  "packageName": "SwiftForAll",
  "name": "SwiftForAll",
  "description": "Generates Swift styleguide on all project types.",
  "version": "0.1.1",
  "author": {
    "name": "Amaury DAVID"
  },
  "options": [
    {
      "name": "Override color structure name",
      "type": "text",
      "id": "colorStructName",
      "default": ""
    },
    {
      "name": "Prefer hex over rgb",
      "type": "switch",
      "id": "colorPreferHex",
      "default": false
    },
    {
      "name": "Font format in text styles",
      "type": "picker",
      "id": "fontFormat",
      "options": [
        {
          "name": "System",
          "value": "system"
        },
        {
          "name": "SwiftGen",
          "value": "swiftgen"
        }
      ],
      "default": "system"
    }
  ],
  "projectTypes": [
    "android",
    "ios",
    "osx",
    "web"
  ],
  "moduleURL": "./main.00c114cb.js",
  "repository": "https://github.com///github.com/amaurydavid/SwiftForAll",
  "readmeURL": "./README.md"
}