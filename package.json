{
  "name": "SwiftForAll",
  "version": "0.1.0",
  "description": "Swift snippets available on all project types!",
  "scripts": {
    "start": "zem start",
    "build": "zem build",
    "clean": "zem clean",
    "exec": "zem exec",
    "publish": "zem publish"
  },
  "zeplin": {
    "displayName": "Swift",
    "projectTypes": [
      "android",
      "ios",
      "osx",
      "web"
    ],
    "options": [
      {
        "name": "Color structure name",
        "type": "text",
        "id": "colorStructName",
        "default": "MyProject"
      }
    ]
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "zem": "^0.3.3"
  }
}
