{
  "name": "ThePreciousExtension",
  "version": "0.1.0",
  "description": "One extension to rule them all. Get Swift & Kotlin snippets on both Android & iOS projects.",
  "scripts": {
    "start": "zem start",
    "build": "zem build",
    "clean": "zem clean",
    "exec": "zem exec",
    "publish": "zem publish"
  },
  "zeplin": {
    "displayName": "ThePreciousExtension",
    "projectTypes": [
      "android",
      "ios"
    ],
    "options": [
      {
        "name": "Always show Swift snippets",
        "type": "switch",
        "id": "alwaysShowSwift",
        "default": false
      },
      {
        "name": "Always show XML snippets",
        "type": "switch",
        "id": "alwaysShowXML",
        "default": false
      },
      {
        "name": "Swift color structure name",
        "type": "text",
        "id": "swiftColorStructName",
        "default": "MyProject"
      }
    ]
  },
  "dependencies": {
    "zem": "^0.3.3"
  }
}
